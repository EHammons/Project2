  <div class="container">
    <div class="title">
      <h1><i class="far fa-eye"></i> SEEK THE TRUTH</h1>
      <h3>ACCEPT NOTHING LESS</h3>
    </div>
    <hr>
    <div class="theories">
      <h3 class="header">Theories:</h3>

      <ul id="example-list" class="list-group">

        {{#each examples}}
        <li data-id="{{this.id}}" class="list-group-item">
          <a href="example/{{this.id}}">{{this.text}}</a>
          <button class="btn btn-danger float-right delete">ï½˜</button>
        </li>
        {{/each}}
      </ul>
    </div>
    <div class="form">
      <form class="clearfix mb-4" action="POST">
        <div class="form-group">
          <h3 class="header">Post a New Theory:<i class="fas fa-eye hidden"></i><i class="fas fa-eye hidden"></i></h3>
          <label for="example-text">Theory Title</label>
          <input type="text" id="example-text" class="form-control" aria-describedby="example-text" placeholder="Enter Title Text">
        </div>
        <div class="form-group">
          <label for="example-description">Theory Description</label>
          <textarea class="form-control" id="example-description" aria-describedby="example-description" placeholder="Enter Theory Description" rows="7"></textarea>
        </div>
        <div class="form-group">
          <label for="example-source">Source</label>
          <input value="100" type="text" id="example-source" class="form-control" aria-describedby="example-source" placeholder="Enter Source Website">
        </div>
          <div class="form-group">
          <label for="example-tag">Tags</label>
          <input type="text" id="example-tag" class="form-control" aria-describedby="example-tag" placeholder="ex: alien, ufo, political, environment">
        </div>
        {{!-- <p id="checkSource" class="btn btn-success float-left">Check Your Source</p> --}}
        <button id="submit" class="btn btn-primary float-right">Submit</button>
      </form>
    </div>
  </div>
</div>



<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="../public/js/index.js"></script>
<script src="/js/index.js"></script>